{% extends '::base.html.twig' %}

{% block bread %}{{ parent() }} <li>Contact</li>{% endblock %}

{% block body %}
    {% form_theme form 'bootstrap_3_layout.html.twig' %}

    <div class="container container-sr">

        <div class="about-text">
            {{ block_content('about.contact') }}
        </div>

    </div>

    <div class="container-sr-odd">
        <div class="container">

            <h3 class="primary-hr-i">Send <em>A Message</em></h3>
            <div class="row">
                <div class="col-sm-9">
                    {{ form_start(form) }}{{  form_end(form) }}
                </div>
            </div>

        </div>
    </div>

{% endblock %}
