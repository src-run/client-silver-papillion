{
  "name": "src-run/web-app",
  "license": "MIT",
  "type": "project",
  "homepage": "https://src.run/augustus-library",
  "description" : "primary application for Source Consulting website",
  "support" : {
    "email": "rmf@src.run",
    "issues" : "https://src.run/web-app/issues",
    "source" : "https://src.run/web-app/git"
  },
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage" : "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "" : "src/"
    },
    "classmap" : [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "require" : {
    "php": "^7.0",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "doctrine/orm": "^2.5",
    "egeloen/ckeditor-bundle": "^4.0",
    "friendsofsymfony/user-bundle": "^2.0",
    "incenteev/composer-parameter-handler": "^2.1",
    "javiereguiluz/easyadmin-bundle": "^1.14",
    "knplabs/knp-menu-bundle": "^2.1",
    "oneup/flysystem-bundle": "1.4.1",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0",
    "src-run/augustus-library": "^0.6",
    "src-run/wonka-bundle": "^0.6",
    "stof/doctrine-extensions-bundle": "^1.2",
    "symfony/assetic-bundle": "^2.8",
    "symfony/cache": "^3.1",
    "symfony/monolog-bundle": "^2.11",
    "symfony/swiftmailer-bundle": "^2.3",
    "symfony/symfony": "^3.1",
    "symfony/routing": "^3.2",
    "vich/uploader-bundle": "^1.2",
    "liip/imagine-bundle": "^1.6",
    "facebook/php-sdk-v4": "~5.0",
    "hostnet/form-handler-bundle": "~1.1",
    "knplabs/knp-paginator-bundle": "~2.5",
    "src-run/augustus-file-library": "~0.6"
  },
  "require-dev" : {
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "doctrine/doctrine-migrations-bundle": "^1.1",
    "sensio/generator-bundle": "^3.0",
    "symfony/phpunit-bridge": "^3.0",
    "phpunit/phpunit": "^5.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "scripts" : {
    "post-install-cmd" : [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd" : [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ]
  },
  "minimum-stability" : "dev",
  "prefer-stable": true,
  "config" : {
    "bin-dir" : "bin",
    "sort-packages" : true
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install" : "relative",
    "incenteev-parameters": {
      "file" : "app/config/parameters.yml"
    },
    "branch-alias" : {
      "dev-master" : "1.0-dev"
    }
  }
}
